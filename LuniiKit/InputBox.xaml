<Window x:Class="LuniiKit.InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LuniiKit"
        mc:Ignorable="d"
        Title="Options de lancement" Height="105" Width="300" Topmost="True" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" WindowStyle="SingleBorderWindow" Icon="/Images/spg.png">
    <Grid>
        <StackPanel>
            <TextBox x:Name="ResponseTextBox" Margin="5,2,5,0" PreviewKeyDown="OKButton_Click" />
            <Button Content="OK" Click="OKButton_Click" Width="200" Margin="0,10,0,0" />
        </StackPanel>
    </Grid>
</Window>
